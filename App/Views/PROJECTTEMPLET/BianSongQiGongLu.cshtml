                if (rowidx == 0) {
                    //需要合并的列
                }

                //测量点 需要下拉框
                htmlString.push(SetTDHtml(rowspan, "OUTPUTVALUE", HangHaoID, rowidx, txtVal, "classstyle120"));
                //二次输入标准值
                htmlString.push(SetTDHtml(rowspan, "READVALUE", HangHaoID, rowidx, txtVal, "classstyle100", unit, 'blurErCiShuRuBiaoZhunZhi'));
