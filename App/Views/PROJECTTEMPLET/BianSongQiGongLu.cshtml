            if (level == "0.0" || ShijiShuChu == "" || BiaoZhunShuChu == "") return ""
            else
                return (((parseFloat(ShijiShuChu) - parseFloat(BiaoZhunShuChu)) / (datas.outputMaxValue - datas.outputMinValue) * 100 / parseFloat(level)) * parseFloat(level)).toFixed(length + 1);
