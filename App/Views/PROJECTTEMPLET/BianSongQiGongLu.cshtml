            var datas = GetValue();
            if ($.trim(erciBiaoZhun) != "" && datas.inputMinValue != "" && datas.inputMaxValue != "" && datas.outputMinValue != "" && datas.outputMaxValue != "")
                return (((datas.inputMaxValue - datas.inputMinValue) == 0 ? 0 : (parseFloat(erciBiaoZhun) - datas.inputMinValue) / (datas.inputMaxValue - datas.inputMinValue)) * (datas.outputMaxValue - datas.outputMinValue) + datas.outputMinValue).toFixed(2)
            else
                return "";
        }
