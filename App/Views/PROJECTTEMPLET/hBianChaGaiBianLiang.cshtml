@section JS {
    <script type="text/javascript" language="javascript">
        //离开事件，用于计算
        function changeValue(obj) {
           
            //重新计算当前行
            var name = $(obj).attr("name");
            var id = $(obj).attr("id");
            id = id.substring(id.indexOf('_'));
            var tongdao = id.split('_')[1];
            //|ymax-ymin|是九个数最大值减去最小值，然后取绝对值。小数点后四位，四舍六入，逢五奇进偶不进
           
            var els = $("#tbody_" + tongdao + "  .YMAX1");
            var max = els[0].value;
            if (max != "") {
                max = parseFloat(max);
            }
            //循环取最大值
            for (var i = 0, j = els.length; i < j; i++) {
                var val = els[i].value;
                if (val != "") {
                    val = parseFloat(val);
                    // val = Math.abs(val);
                    if (max == "") {
                        max = val;
                    } else if (max < val)
                    { max = val; }
                }

            }
            //【最小指示值Umin(V)】为14个数据最小值	
            var min = els[0].value;
            if (min != "") {
                min = parseFloat(min);
            }
            //循环取最小值
            for (var i = 0, j = els.length; i < j; i++) {
                var val = els[i].value;
                if (val != "") {
                    val = parseFloat(val);
                    // val = Math.abs(val);
                    if (min == "") {
                        min = val;
                    } else if (min > val)
                    { min = val; }
                }

            }
            var YMAXvalue = Math.abs(max - min);
            YMAXvalue = zeroFloat(fomatFloat(YMAXvalue, 4), 4);
            $("#YMAX6" + "_" + tongdao + "_1_1").val(YMAXvalue);


           var els = $("#tbody_" + tongdao + "   .YMAX2");
           var max = els[0].value;
           if (max != "") {
               max = parseFloat(max);
           }
            //循环取最大值
           for (var i = 0, j = els.length; i < j; i++) {
               var val = els[i].value;
               if (val != "") {
                   val = parseFloat(val);
                   // val = Math.abs(val);
                   if (max == "") {
                       max = val;
                   } else if (max < val)
                   { max = val; }
               }

           }
            //【最小指示值Umin(V)】为10个数据最小值	
           var min = els[0].value;
           if (min != "") {
               min = parseFloat(min);
           }
            //循环取最小值
           for (var i = 0, j = els.length; i < j; i++) {
               var val = els[i].value;
               if (val != "") {
                   val = parseFloat(val);
                   // val = Math.abs(val);
                   if (min == "") {
                       min = val;
                   } else if (min > val)
                   { min = val; }
               }

           }
           var YMINvalue = Math.abs(max - min);
           YMINvalue = zeroFloat(fomatFloat(YMINvalue, 4), 4);
           $("#YMAX6" + "_" + tongdao + "_1_3").val(YMINvalue);
            //Δymax=是绝对值中的最大值										
           var ymax = parseFloat(YMAXvalue) > parseFloat(YMINvalue) ? YMAXvalue : YMINvalue;

           $("#ymax" + "_" + tongdao + "_1_1").val(ymax);
        }
       
      
    </script>

}

